{
    "manifest_version": 2,
   
    "name": "Antitube",
    "description": "youtube blocker",
    "version": "1.0",
   
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab",
      "storage"
    ],
    "content_scripts": [ 
        { 
            "js": [ "jquery-3.4.1.min.js", "script.js"],
             "matches": [ "http://*/*", "https://*/*"]
         }
     ]
  
  }